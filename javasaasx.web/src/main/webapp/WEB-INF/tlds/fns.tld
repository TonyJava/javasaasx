  <display-name>JSTL functions sys</display-name>
  <tlib-version>1.1</tlib-version>
  <short-name>fns</short-name>
  <uri>http://java.sun.com/jsp/jstl/functionss</uri>

  <!-- UserUtil -->
  <function>
    <description>获取 用户下拉列表数据</description>
    <name>getUserSelectListData</name>
    <function-class>com.choudou5.javasaasx.base.util.UserUtil</function-class>
    <function-signature>java.util.List getUserSelectListData(java.lang.String))</function-signature>
    <example>${fns:getUserSelectListData(userId)}</example>
  </function>


