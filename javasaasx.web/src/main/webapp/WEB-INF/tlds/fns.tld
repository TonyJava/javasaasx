  <!-- UIUtil -->

  <!-- GenUtil -->
  <function>
    <description> 获取 生成页面类型 枚举列表</description>
    <name>genPageTypeEnums</name>
    <function-class>com.choudou5.javasaasx.service.impl.util.GenUtil</function-class>
    <function-signature>com.choudou5.javasaasx.service.constants.GenConstants.GenPageTypeEnum[] genPageTypeEnums()</function-signature>
    <example>${fns:genPageTypeEnums()}</example>
  </function>

  <!-- StrUtil -->

  <!-- GzipUtil -->
  <function>
    <description>Gzip解压</description>
    <name>gzipUnCompress</name>
    <function-class>com.choudou5.base.util.GzipUtil</function-class>
    <function-signature>java.lang.String uncompress(java.lang.String)</function-signature>
    <example>${fns:gzipUnCompress(str)}</example>
  </function>

  <!-- DateUtil -->
  <function>
    <description>格式化时间 yyyy-MM-dd HH:mm:ss</description>
    <name>formatDateTime</name>
    <function-class>cn.hutool.core.date.DateUtil</function-class>
    <function-signature>java.lang.String formatDateTime(java.util.Date)</function-signature>
    <example>${fns:formatDateTime(date)}</example>
  </function>
  <function>
    <description>格式化时间 yyyy-MM-dd</description>
    <name>formatDate</name>
    <function-class>cn.hutool.core.date.DateUtil</function-class>
    <function-signature>java.lang.String formatDate(java.util.Date)</function-signature>
    <example>${fns:formatDate(date)}</example>
  </function>
  <function>
    <description>格式化时间 HH:mm:ss</description>
    <name>formatTime</name>
    <function-class>cn.hutool.core.date.DateUtil</function-class>
    <function-signature>java.lang.String formatTime(java.util.Date)</function-signature>
    <example>${fns:formatTime(date)}</example>
  </function>

