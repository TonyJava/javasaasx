        )
     </insert>

    <insert id="batchInsert" keyProperty="id">
        INSERT INTO gen_table_column_style (
        `id`
        ,`table`
        ,`column`
        ,`desc`
        ,`field_name`
        ,`is_insert`
        ,`is_edit`
        ,`is_list`
        ,`is_query`
        ,`query_type`
        ,`show_type`
        ,`dic_type`
        ,`sort`
        ) VALUES
        <foreach collection="entities" item="entity" index="index" separator="," >
        (
        #{entity.id}
        ,#{entity.table}
        ,#{entity.column}
        ,#{entity.desc}
        ,#{entity.fieldName}
        ,#{entity.isInsert}
        ,#{entity.isEdit}
        ,#{entity.isList}
        ,#{entity.isQuery}
        ,#{entity.queryType}
        ,#{entity.showType}
        ,#{entity.dicType}
        ,#{entity.sort}
        )
        </foreach>
    </insert>

    <update id="update" >
