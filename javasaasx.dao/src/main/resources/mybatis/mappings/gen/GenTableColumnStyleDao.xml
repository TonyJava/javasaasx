    <sql id="where">
        <where>
            <!--
            eg: AND a.title LIKE concat('%',#{paramBean.title},'%')
            -->
            <if test="paramBean != null and paramBean.id != null and paramBean.id != ''">
                AND `id` = #{paramBean.id}
            </if>
            <if test="paramBean != null and paramBean.table != null and paramBean.table != ''">
